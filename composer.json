{
    "name": "kainotomo/phmoney_cms",
    "type": "library",
    "description": "PHMoney Double-Entry Accounting Program laravel cms.",
    "keywords": [
        "laravel",
        "accounting",
        "invoice",
        "software",
        "online"
    ],
    "license": "GPL-3.0+",
    "support": {
        "issues": "https://github.com/kainotomo/phmoney/issues",
        "source": "https://github.com/kainotomo/phmoney"
    },
    "authors": [
        {
            "name": "KAINOTOMO PH LTD",
            "email": "info@kainotomo.com"
        }
    ],
    "require": {
        "laravel/jetstream": "^2.8",
        "league/csv": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kainotomo\\PHMoney\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kainotomo\\PHMoney\\PHMoneyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimun-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php artisan phmoney_provider:install",
            "@php artisan phmoney_provider:update",
            "@php artisan migrate"
        ],
        "post-update-cmd": [
            "@php artisan phmoney_provider:update"
        ]
    }
}
